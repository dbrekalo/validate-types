{
  "name": "validate-types",
  "version": "1.0.2",
  "description": "Validate object properties against type schema",
  "author": {
    "email": "dbrekalo@gmail.com",
    "name": "Damir Brekalo"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbrekalo/validate-types"
  },
  "scripts": {
    "docs:dev": "vuepress dev documentation",
    "docs:build": "vuepress build documentation",
    "build": "npm run build:lib && npm run docs:build",
    "build:lib": "rollup --config",
    "lint": "eslint index.js test/index.js --color",
    "test": "npm run lint && npm run test:lib",
    "test:lib": "nyc --reporter=lcov mocha ./test/index.js",
    "watch:test:lib": "nyc --reporter=lcov mocha -w ./test/index.js"
  },
  "keywords": [
    "validate",
    "validator",
    "types",
    "type-checker"
  ],
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "chai": "^4.1.2",
    "eslint": "^7.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.2.0",
    "nyc": "^15.0.1",
    "rollup": "^2.11.2",
    "rollup-plugin-terser": "^6.1.0",
    "vuepress": "^1.5.0",
    "watchpack": "^1.6.1"
  },
  "dependencies": {}
}
