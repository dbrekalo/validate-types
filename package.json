{
  "name": "validate-types",
  "version": "1.0.1",
  "description": "Validate object properties against type schema",
  "author": {
    "email": "dbrekalo@gmail.com",
    "name": "Damir Brekalo"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbrekalo/type-factory"
  },
  "scripts": {
    "watch": "grunt",
    "build": "grunt build",
    "test": "mocha --reporter spec",
    "watch:test": "mocha --watch --reporter spec",
    "coverage": "istanbul cover _mocha -- -R spec",
    "coverageReport": "npm run coverage && open coverage/lcov-report/*.html",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "bump:patch": "grunt bump:patch",
    "bump:minor": "grunt bump:minor",
    "bump:major": "grunt bump:major"
  },
  "keywords": [
    "validate",
    "validator",
    "types",
    "type-checker"
  ],
  "devDependencies": {
    "attire": "^1.3.9",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "grunt": "^1.0.3",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^3.4.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^21.0.0",
    "istanbul": "^0.4.5",
    "load-grunt-tasks": "^4.0.0",
    "mocha": "^5.2.0"
  }
}
